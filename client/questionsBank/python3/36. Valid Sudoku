class Solution:
    def isValidSudoku(self, board: List[List[str]]) -> bool:
        rows = defaultdict(list)
        cols = defaultdict(list)
        rects = defaultdict(list)

        R = len(board)
        C = len(board[0])

        for i in range(R):
            for j in range(C):
                if board[i][j] == ".":
                    continue

                val = board[i][j]
                if val in rows[i]:
                    return False
                rows[i].append(val)

                if val in cols[j]:
                    return False

                cols[j].append(val)

                if val in cols[(i//3, j//3)]:
                    return False

                cols[(i//3, j//3)].append(val)

        return True
